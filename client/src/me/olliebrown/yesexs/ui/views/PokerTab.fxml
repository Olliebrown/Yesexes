                  <Label fx:id="m30" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="0.0" GridPane.columnIndex="0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3" GridPane.vgrow="ALWAYS" />
                  <Label fx:id="m31" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="0.0" GridPane.columnIndex="1" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3" GridPane.vgrow="ALWAYS" />
                  <Label fx:id="m32" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="0.0" GridPane.columnIndex="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3" GridPane.vgrow="ALWAYS" />
                  <Label fx:id="m33" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="1.0" GridPane.columnIndex="3" GridPane.hgrow="ALWAYS" GridPane.rowIndex="3" GridPane.vgrow="ALWAYS" />
               </children>
               <columnConstraints>
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
                  <ColumnConstraints />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints />
                  <RowConstraints />
                  <RowConstraints />
                  <RowConstraints />
               </rowConstraints>
            </GridPane>
         </TitledPane>
      </VBox>
   </HBox>
   <HBox alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308" spacing="10.0" VBox.vgrow="ALWAYS">
      <Region maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS" />
      <Label text="%poker.transform.poke_address" />
      <AddressSpinner fx:id="pokeAddress" editable="true" />
      <Button mnemonicParsing="false" onAction="#poke" text="%poker.transform.poke" />
   </HBox>

   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>

   <fx:define>
      <ArrayList fx:id="translate">
         <fx:reference source="translateX" />
         <fx:reference source="translateY" />
         <fx:reference source="translateZ" />
      </ArrayList>

      <ArrayList fx:id="rotate">
         <fx:reference source="rotateX" />
         <fx:reference source="rotateY" />
         <fx:reference source="rotateZ" />
      </ArrayList>

      <ArrayList fx:id="matrixLabels">
         <fx:reference source="m00" />
